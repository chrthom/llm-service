{
  "name": "llm-gateway",
  "version": "0.1.0",
  "description": "Socket.io based gateway for LLMs",
  "main": "dist/app.js",
  "type": "module",
  "scripts": {
    "s": "npm run start",
    "start": "npx nodemon --exec 'node --experimental-specifier-resolution=node --loader ts-node/esm' src/app.ts",
    "test": "npx jest",
    "lint": "npx eslint src/*",
    "format": "npx prettier . --write"
  },
  "private": true,
  "author": "Christopher Thomsen",
  "dependencies": {
    "@llama-node/core": "0.1.5",
    "config": "^3.0.0",
    "cors": "^2.0.0",
    "express": "^4.0.0",
    "llama-node": "0.1.5",
    "node-fetch": "^2.0.0",
    "nodemon": "^2.0.0",
    "socket.io": "^4.0.0"
  },
  "devDependencies": {
    "@types/config": "^3.0.0",
    "@types/express": "^4.0.0",
    "@types/jest": "^29.0.0",
    "@types/node": "^18.0.0",
    "@types/node-fetch": "^2.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.0.0",
    "eslint-plugin-prefer-arrow": "~1.2.0",
    "jest": "^29.0.0",
    "prettier": "^3.0.0",
    "ts-jest": "^29.0.0",
    "ts-node": "^10.0.0",
    "tslib": "^2.0.0",
    "typescript": "~5.1.0"
  }
}
